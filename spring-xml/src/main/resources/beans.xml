<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="factoryService" class="pawg.cmd.beans.FactoryService" />
    <bean id="planeFactory" class="pawg.cmd.beans.PlaneFactory" factory-method="getInstance"/>
    <bean id="printService" class="pawg.cmd.beans.PrintService" init-method="awesomeInit" scope="prototype"/>

    <bean id="zelaznyMordulec" class="pawg.cmd.beans.EngineFactory">
        <constructor-arg ref="factoryService" />
        <constructor-arg ref="planeFactory" />
        <property name="printService" ref="printer" />
    </bean>

    <alias name="printService" alias="printer" />

</beans>